MODULE main;


::yak CLASS
{
   .z VAR -> I32;
   .f VAR =>> I32;
   .f_count VAR SZ;

   .DESTRUCTOR FUNCTION()
   {
     IF (.z??)
     {
        .z-> := .z-> + 1;
     }

     IF (.f??)
     {
        WHILE(.f_count > 0)
        {
           .f-> ++;
           .f_count := .f_count - 1;
        }
     }
   }
}